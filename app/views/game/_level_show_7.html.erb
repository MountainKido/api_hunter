<% content_for :js do %>
  <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&libraries=geometry"></script>
  <script>
var map , panorama;

function initialize() {
  var is_error = false;
  try{
    <%= raw(@message) %>
  }catch(e){
    console.log(e);
    is_error = true;
  }

  send_success(!is_error && map && panorama, <%= @score.id %>);
  
}
google.maps.event.addDomListener(window, 'load', initialize);
    </script>
<% end %>

<pre id='map_info'></pre>
<div class='row' style="margin-bottom:300px;">
  <div id="google_map" style="width:30%;height:400px;float:left;"></div>
  <div id="panorama" style="width:69%;height:400px;float:right;"></div>
</div>
