<% content_for :js do %>
  <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
  <script>
var panorama;

function initialize() {
  var is_error = false;
  try{
    <%= raw(@message) %>
  }catch(e){
    console.log(e);
    is_error = true;
  }

  send_success(!is_error && panorama, <%= @score.id %>);
  
}
google.maps.event.addDomListener(window, 'load', initialize);
    </script>
<% end %>

<div id="panorama" style="width:100%;height:400px;margin-bottom:300px;"></div>
